discrepancy = int((cold_water_cost_flat - cold_water_cost_room1 - cold_water_cost_room2 - cold_water_cost_room3 - cold_water_cost_room4) / 4 + (hot_water_cost_flat - hot_water_cost_room1 - hot_water_cost_room2 - hot_water_cost_room3 - hot_water_cost_room4) / 4 + (water_removal_cost_flat - water_removal_cost_room1 - water_removal_cost_room2 - water_removal_cost_room3 - water_removal_cost_room4)/ 4)